                <TextBox.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="25"/>
                    </Style>
                </TextBox.Resources>
            </TextBox>
            <TextBox x:Name="CommonTags_txt_Copy" Height="132" Canvas.Left="41" TextWrapping="Wrap" Text="Likes and Dislikes:&#xA;&#xA;I like to chat about anything that comes to mind. I enjoy walks along the seaside." Canvas.Top="227" Width="337" FontSize="20" Foreground="#DDFFFFFF"/>
