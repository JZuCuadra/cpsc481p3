                <TextBox.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="25"/>
                    </Style>
                </TextBox.Resources>
            </TextBox>
            <TextBox x:Name="TagItem02_txt"   Height="38" Canvas.Left="205" TextWrapping="Wrap" Text="Engineer" Canvas.Top="127" Width="115" FontSize="20" TextDecorations="{x:Null}" Background="#FF006064" Foreground="#DDFFFFFF" TextAlignment="Center" BorderBrush="#00000000">
